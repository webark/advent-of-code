{
  "name": "advent-of-code",
  "version": "2.0.0",
  "description": "A template for solving of Advent of Code in JavaScript",
  "repository": "git@github.com:webark/advent-of-code.git",
  "author": "Mark Avery <mark@webark.cc>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "new": "mkdir -p ./solutions/$YEAR/$DAY && cp -r ./_template/ ./solutions/$YEAR/$DAY/",
    "solution": "node ./solution.js $YEAR $DAY",
    "test": "node ./solutions/$YEAR/$DAY/test.js"
  },
  "devDependencies": {
    "colors": "^1.4.0",
    "uvu": "^0.5.6"
  },
  "packageManager": "yarn@3.3.0"
}
